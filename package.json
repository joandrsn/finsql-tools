{
    "name": "dynamics-nav-scm",
    "displayName": "dynamics-nav-scm",
    "description": "",
    "version": "0.0.1",
    "publisher": "Spuc",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World",
                "category": "Dynamics NAV SCM"
            },
            {
                "command": "dynamicsnavscm.relaunchterminal",
                "title": "Relaunch terminal",
                "category": "Dynamics NAV SCM"
            },
            {
                "command": "dynamicsnavscm.startide",
                "title": "Start NAV Development Enviroment",
                "category": "Dynamics NAV SCM"
            },
            {
                "command": "dynamicsnavscm.exportfromnav",
                "title": "Export objects from NAV",
                "category": "Dynamics NAV SCM"
            },
            {
                "command": "dynamicsnavscm.importtonav",
                "title": "Import objects to NAV",
                "category": "Dynamics NAV SCM"
            },
            {
                "command": "dynamicsnavscm.generategitattributes",
                "title": "Generate gitattributes-file",
                "category": "Dynamics NAV SCM"
            }
        ],
        "configuration": [
            {
                "type": "object",
                "title": "Dynamics NAV configuration",
                "properties": {
                    "dynamicsnavscm.rtcpath": {
                        "type": "string",
                        "default": "",
                        "description": "stuff"
                    },
                    "dynamicsnavscm.nstpath": {
                        "type": "string",
                        "default": ""
                    },
                    "dynamicsnavscm.databasename": {
                        "type": "string",
                        "default": ""
                    },
                    "dynamicsnavscm.databaseserver": {
                        "type": "string",
                        "default": "localhost"
                    },
                    "dynamicsnavscm.compileafterimport": {
                        "type": "boolean",
                        "default": true
                    },
                    "dynamicsnavscm.exportunlicensedasbinary": {
                        "type": "boolean",
                        "default": true
                    },
                    "dynamicsnavscm.exportfilters": {
                        "type": "array"
                    },
                    "dynamicsnavscm.uidoffset": {
                        "type": "integer",
                        "default": 50000
                    },
                    "dynamicsnavscm.datemodification": {
                        "type": "boolean",
                        "default": true
                    }

                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm run powershell && tsc -p ./",
        "watch": "npm run powershell && tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "powershell": "powershell -noprofile -file ./psscripts/copyps.ps1",
        "updatepsmodules": "powershell -noprofile -file ./psscripts/update_modules.ps1"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}